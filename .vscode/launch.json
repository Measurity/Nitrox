            "name": "Run Nitrox.Launcher",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-launcher",
            // If you have changed target frameworks, make sure to update the pro gram path.
            "program": "${workspaceFolder}/Nitrox.Launcher/bin/Debug/Nitrox.Launcher.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Nitrox.Launcher",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "internalConsole",
            "stopAtEntry": false,
            "sourceFileMap": { 
